{
  "name": "fetal-recon-ui-mvp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "fmt": "biome format --write .",
    "dicom-push": "bun run ./testing/dicomPush.mjs",
    "wipe": "./testing/miniChRIS/scripts/clear_pacsfiles.sh",
    "minichris:up": "cd testing/miniChRIS && ./minichris.sh chris db oxidicom graphql-engine",
    "minichris:down": "cd testing/miniChRIS && ./unmake.sh",
    "setup": "bun run minichris:up && bun run setup:scripts",
    "setup:scripts": "bun run download-dicoms && bun run add-pipeline",
    "setup:add-pipeline": "bun run testing/addPipeline.mjs",
    "setup:download-dicoms": "bun run testing/downloadData.mjs",
    "setup:hasura": "hasura metadata apply --project testing/hasura"
  },
  "devDependencies": {
    "@apollo/client": "^3.11.8",
    "@biomejs/biome": "1.9.4",
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/kit": "^2.7.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/bun": "^1.1.11",
    "graphql": "^16.9.0",
    "jose": "^5.9.6",
    "svelte": "^5.0.5",
    "svelte-apollo": "^0.5.0",
    "svelte-check": "^4.0.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.9"
  },
  "type": "module"
}
